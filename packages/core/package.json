{
  "name": "@mamdouh-aboammar/pixolink",
  "version": "1.0.0",
  "description": "PixoLink Core - Unified SDK orchestrator",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./ui": {
      "import": "./dist/ui.mjs",
      "require": "./dist/ui.js",
      "types": "./dist/ui.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src --ext .ts,.tsx",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "keywords": [
    "sdk",
    "ai",
    "payments",
    "security",
    "admin",
    "supabase",
    "pixora"
  ],
  "author": "PixoRA Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/pixora/pixolink.git",
    "directory": "packages/core"
  },
  "publishConfig": {
    "access": "public"
  }
}
