{
  "name": "@mamdouh-aboammar/pixolink-pixopay",
  "version": "1.0.0",
  "description": "Payment processing module for PixoLink SDK with Egyptian gateways (Instapay, VF Cash) and Stripe",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "lint": "eslint src/",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "pixolink",
    "payment",
    "instapay",
    "vodafone-cash",
    "stripe",
    "egypt",
    "payment-gateway",
    "transactions"
  ],
  "dependencies": {
    "@mamdouh-aboammar/pixolink": "workspace:*",
    "stripe": "^17.4.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pixora/pixolink.git",
    "directory": "packages/modules/pixopay"
  },
  "license": "MIT"
}
