{
  "name": "@pixora/pixsync",
  "version": "1.0.0",
  "description": "Cross-environment sync and conflict resolution for PixoRA",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest",
    "test:ui": "vitest --ui"
  },
  "keywords": ["pixora", "sync", "conflict-resolution", "offline-first"],
  "author": "PixoRA Team",
  "license": "PROPRIETARY",
  "peerDependencies": {
    "@supabase/supabase-js": "^2.0.0"
  },
  "dependencies": {
    "dexie": "^4.0.0"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^2.0.0"
  }
}
